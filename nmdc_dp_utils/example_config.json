{
    "study": {
        "name": "example_study",
        "id": "nmdc:sty-11-example",
        "description": "Example NMDC metabolomics study configuration demonstrating metadata override patterns"
    },
    "workflow": {
        "name": "example_study_lcms_metab",
        "massive_id": "v07/MSV000012345",
        "file_type": ".raw",
        "file_filters": ["pos", "neg", "hilic", "rp"],
        "processed_data_date_tag": "20251111",
        "workflow_type": "LCMS Metabolomics",
        "batch_size": 25
    },
    "paths": {
        "base_directory": "/path/to/base/directory",
        "data_directory": "/path/to/data"
    },
    "docker": {
        "raw_data_inspector_image": "microbiomedata/metams:3.3.3"
    },
    "minio": {
        "endpoint": "admin.nmdcdemo.emsl.pnl.gov",
        "secure": true,
        "bucket": "metabolomics",
        "enabled": true,
        "processed_data_folder": "{study_name}/LC-MS/{config_name}/"
    },
    "configurations": [
        {
            "name": "hilic_pos",
            "file_filter": ["hilic", "pos"],
            "corems_toml": "/path/to/hilic_pos_corems.toml",
            "reference_db": "/path/to/database.msp",
            "scan_translator": "/path/to/scan_translator.toml",
            "cores": 5,
            "chromat_configuration_name": "HILIC",
            "mass_spec_configuration_name": "ESI Orbitrap MS/MS Positive",
            "metadata_overrides": {
                "mass_spec_configuration_name": {
                    "CE102040": "JGI/LBNL Standard Metabolomics Method, positive @10,20,40CE",
                    "CE205060": "JGI/LBNL Standard Metabolomics Method, positive @20,50,60CE"
                }
            }
        },
        {
            "name": "hilic_neg",
            "file_filter": ["hilic", "neg"],
            "corems_toml": "/path/to/hilic_neg_corems.toml",
            "reference_db": "/path/to/database.msp",
            "scan_translator": "/path/to/scan_translator.toml",
            "cores": 5,
            "chromat_configuration_name": "HILIC",
            "mass_spec_configuration_name": "ESI Orbitrap MS/MS Negative",
            "metadata_overrides": {
                "mass_spec_configuration_name": {
                    "CE102040": "JGI/LBNL Standard Metabolomics Method, negative @10,20,40CE",
                    "CE205060": "JGI/LBNL Standard Metabolomics Method, negative @20,50,60CE"
                }
            }
        },
        {
            "name": "rp_pos",
            "file_filter": ["rp", "pos"],
            "corems_toml": "/path/to/rp_pos_corems.toml", 
            "reference_db": "/path/to/database.msp",
            "scan_translator": "/path/to/scan_translator.toml",
            "cores": 5,
            "chromat_configuration_name": "C18",
            "mass_spec_configuration_name": "ESI Orbitrap MS/MS Positive",
            "metadata_overrides": {
                "mass_spec_configuration_name": {
                    "CE102040": "JGI/LBNL Standard Metabolomics Method, positive @10,20,40CE",
                    "CE205060": "JGI/LBNL Standard Metabolomics Method, positive @20,50,60CE"
                }
            }
        },
        {
            "name": "multi_instrument_example",
            "file_filter": ["multi"],
            "corems_toml": "/path/to/multi_corems.toml",
            "reference_db": "/path/to/database.msp",
            "scan_translator": "/path/to/scan_translator.toml",
            "cores": 5,
