                },
                "chromat_configuration_name": {
                    "_HILIC_": "JGI/LBNL Metabolomics Standard LC Method - Polar HILIC-Z",
                    "_C18_": "JGI/LBNL Metabolomics Standard LC Method - Nonpolar C18",
                    "_PFPP_": "JGI/LBNL Metabolomics Standard LC Method - PFPP"
                },
                "mass_spec_configuration_name": {
                    "CE102040_POS": "JGI/LBNL Standard Metabolomics Method, positive @10,20,40CE",
                    "CE102040_NEG": "JGI/LBNL Standard Metabolomics Method, negative @10,20,40CE",
                    "CE205060_POS": "JGI/LBNL Standard Metabolomics Method, positive @20,50,60CE",
                    "CE205060_NEG": "JGI/LBNL Standard Metabolomics Method, negative @20,50,60CE"
                }
            }
        },
        {
            "name": "soil_microbiome_example",
            "file_filter": ["soil"],
            "corems_toml": "/path/to/soil_corems.toml",
